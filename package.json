{
  "private": true,
  "name": "remix-app-template-js",
  "description": "",
  "license": "",
  "scripts": {
    "build": "yarn build:css && remix build",
    "build:css": "tailwindcss -m -i ./app/styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"yarn dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./app/styles/app.css -o app/styles/app.css",
    "postinstall": "remix setup node"
  },
  "dependencies": {
    "@remix-run/react": "^1.1.3",
    "@remix-run/serve": "^1.1.3",
    "@remix-run/vercel": "^1.1.3",
    "front-matter": "^4.0.2",
    "marked": "^4.0.12",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.3",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.3",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.18"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
